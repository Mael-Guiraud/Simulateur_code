plot '../datas/be_distrib.data' smooth cumulative title "BE traffic" 
set term postscript color solid
set xrange [0:200] 
set yrange [0:100] 
set title "Cumulative distribution of the sojourn time"
set xlabel "Latency ({/Symbol m}s)" 
set ytics 10
set key bottom right 
set ylabel "Distribution (%)"
set output '| ps2pdf - ../gnuplot/opti.pdf'
replot
